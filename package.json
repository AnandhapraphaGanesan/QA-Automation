    "androidfirefox": "npm run e2e-test-androidfirefox",
    "simulatorsafari": "npm run e2e-test-simulatorsafari",
    "iossafari": "npm run e2e-test-iossafari",
    "androidapp": "npm run e2e-test-androidapp",
    "iosapp": "npm run e2e-test-iosapp",
    "emulatorapp": "npm run e2e-test-emulatorapp",
    "simulatorapp": "npm run e2e-test-simulatorapp",
    "e2e-test-api": "nightwatch --env api_testing --format --reporter=html",
    "e2e-test-chrome": "nightwatch --env chrome --format --reporter=html",
    "e2e-test-safari": "nightwatch --env safari --format --reporter=html",
    "e2e-test-firefox": "nightwatch --env firefox --format --reporter=html",
    "e2e-test-emulatorchrome": "nightwatch --env android.emulator.chrome --format --reporter=html",
    "e2e-test-emulatorfirefox": "nightwatch --env android.emulator.firefox --format --reporter=html",
    "e2e-test-androidchrome": "nightwatch --env android.real.chrome --format --reporter=html",
    "e2e-test-androidfirefox": "nightwatch --env android.real.firefox --format --reporter=html",
    "e2e-test-simulatorsafari": "nightwatch --env ios.simulator.safari --format --reporter=html",
    "e2e-test-iossafari": "nightwatch --env ios.real.safari --format --reporter=html",
    "e2e-test-androidapp": "nightwatch --env app.android.real --format --reporter=html",
    "e2e-test-iosapp": "nightwatch --env app.ios.real --format --reporter=html",
    "e2e-test-emulatorapp": "nightwatch --env app.android.emulator --format --reporter=html",
    "e2e-test-simulatorapp": "nightwatch --env app.ios.simulator --format --reporter=html"
  },
